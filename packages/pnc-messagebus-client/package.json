{
  "name": "@project-ncl/pnc-messagebus-client",
  "version": "2.2.1",
  "description": "Client library for consuming PNC WebSocket notifications",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/project-ncl/pnc-js-libs.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "source": "src/index.ts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf dist/",
    "nuke": "npm run clean && rm -rf node_modules",
    "lint": "tslint -c ../../tslint.json 'src/**/*.ts'",
    "lint:fix": "tslint -c ../../tslint.json 'src/**/*.ts' --fix",
    "test": "jest",
    "pretest": "npm run lint",
    "compile": "tsc",
    "prebuild": "npm run clean && npm run test",
    "build": "npm run compile"
  },
  "keywords": [
    "PNC",
    "WebSocket",
    "Notification",
    "MessageBus"
  ],
  "author": "Alex Creasy <acreasy@redhat.com>",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/jest": "^24.0.23",
    "@types/ws": "^6.0.4",
    "jest": "^26.6.3",
    "jest-diff": "^26.6.2",
    "jest-websocket-mock": "2.2.0",
    "mock-socket": "^8.1.0",
    "npm-run-all": "^4.1.5",
    "replace": "^1.2.1",
    "ts-jest": "^26.5.4",
    "tslint": "^6.1.3",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "@project-ncl/pnc-dto-types": "^2.2.1"
  },
  "engines": {
    "node": "^20.10.0",
    "npm": "^10.2.5"
  }
}
